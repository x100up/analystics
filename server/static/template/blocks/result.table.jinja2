{% for index in data %}
    {% for seriaIndex in data[index] %}
        <h2>{{ data[index][seriaIndex]['table_name'] }}</h2>
    <table>
    <thead>
        <tr><th>Дата</th><th>Значение</th></tr>
    </thead>
    <tbody>
        {% for date, value in data[index][seriaIndex]['data'] %}
        <tr>
            <td>
            {{ date|smartDate(data[index][seriaIndex]['opt']['interval']) }}
            </td>
            <td>{{ value }}</td>
        </tr>
        {% endfor %}
    </tbody>
    </table>
    {% endfor %}
{% endfor %}
