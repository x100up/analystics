<!DOCTYPE html>
<html>
<head>
    <title>{{ title }}</title>
    <link rel="stylesheet" href="{{ static_url_prefix }}/css/gum.css" type="text/css" media="screen, projection" />
    <!--[if lte IE 6]><link rel="stylesheet" href="{{ static_url_prefix }}/css/gum_ie.css" type="text/css" media="screen, projection" /><![endif]-->
    <link href="{{ static_url_prefix }}/css/reset.css" rel="stylesheet" type="text/css">
    <link href="{{ static_url_prefix }}/css/960.css" rel="stylesheet" type="text/css">
    <link href="{{ static_url_prefix }}/css/button.css" rel="stylesheet" type="text/css">
    <link href="{{ static_url_prefix }}/css/style.css" rel="stylesheet" type="text/css">
    <link rel="icon" type="image/png" href="{{ static_url_prefix }}/img/favicon.png" />
    <script type="text/javascript" src="{{ static_url_prefix }}/js/jquery-1.8.0.min.js"></script>
    {% if js_vars %}
        <script type="text/javascript">
            {% for key in js_vars %}
                var {{ key }} = {{ js_vars[key]|toJsVar }};
            {% endfor %}
        </script>
    {% endif %}
    {% block head %}{% endblock %}
</head>

<body>
    <div id="wrapper">

        <header id="header">
            <div>
                <a href="/" id="logo">
                    <img src="{{ static_url_prefix }}/img/logo.png" alt="Analytics logo">
                </a>

                <a href="/dashboard" class="analytics"></a>

                <ul id="nav">
                    <li><a href="/admin" class="manage"></a></li>
                    <li><a href="/user/logout/" class="logout"></a></li>
                </ul>
            </div>
        {#
            <nav>
                {% if __user__ %}
                    {% include 'blocks/menu.jinja2' %}
                {% endif %}
            </nav>
          #}
        </header><!-- #header-->

        <div id="topPanel">
            <div>
                <span class="selectedApp">TopFace</span>

                <button onclick="startNewTask()" id="addTaskButton"></button>
                <button id="fromTemplateButton"></button>
            </div>


        </div>

        <section id="middle">

            <div id="container">
                <div id="content">
                    {% block content %}{% endblock %}
                </div><!-- #content-->
            </div><!-- #container-->

            <aside id="sideLeft">
                <div>
                    {% block sidebar %}{% endblock %}
                </div>
            </aside><!-- #sideLeft -->

        </section><!-- #middle-->

    </div>
</body>
</html>