<!DOCTYPE html>
<html>
<head>
    <title>{{ title }}</title>
    <link href="{{ static_url_prefix }}/css/reset.css" rel="stylesheet" type="text/css">
    <link href="{{ static_url_prefix }}/css/960.css" rel="stylesheet" type="text/css">
    <link href="{{ static_url_prefix }}/css/button.css" rel="stylesheet" type="text/css">
    <link href="{{ static_url_prefix }}/css/style.css" rel="stylesheet" type="text/css">
    <link rel="icon" type="image/png" href="{{ static_url_prefix }}/img/favicon.png" />
    <script type="text/javascript" src="{{ static_url_prefix }}/js/jquery-1.8.0.min.js"></script>
    {% block head %}{% endblock %}
</head>
<body>
    <div id="wrapper">
        <header class="container_12">
            <a href="/" id="logo">
            <img src="{{ static_url_prefix }}/img/logo.png" alt="Analytics logo">
            </a>
        </header>
        <nav class="container_12">
        {% if __user__ %}
            {% include 'blocks/menu.jinja2' %}
        {% endif %}
        </nav>


        <div class="container_12" id="content">
            {% block content %}{% endblock %}
        </div>
    </div>
</body>
</html>