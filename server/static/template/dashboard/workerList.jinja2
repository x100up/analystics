{% for worker in lastWorkers %}
    <div class="job job_ALIVE a_{{worker.status}}" id="worker_row_{{worker.workerId}}" data-workerid="{{worker.workerId}}">

        <div class="q ALIVE a_{{ worker.status }}">
            <input type="checkbox" value="{{worker.workerId}}" name="jobId" class="jobcb">
        </div>
        <div class="main" title="{{worker.startDate|smartDateInterval(worker.endDate)}}" onclick="loadResult({{worker.workerId}})">
            <span class="a" id="link_{{worker.workerId}}">{{worker.name}}</span>
            <em>Запущена: {{worker.startDate|datetofiled}}</em>
        </div>
        <div class="plus">
            <span onclick="startNewTask({{worker.workerId}})" class="plus" title="create on base"></span>
            <span class="status"><img src="/static/img/loader.gif" alt=""></span>
        </div>
    </div>
{% endfor %}